{
    "name": "alexgh12/changelog-laravel",
    "description": "Registro de cambios de proyectos en Laravel",
    "keywords": ["changelog","CHANGE.md","in-laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Gh",
            "email": "a-l-e-x_1379@hotmail.com"
        }
    ],
    "require": {
        "php": "^7.4|^8.0",
        "laravel/framework": "^7.0|^8.37|^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AlexGh12\\ChangeLog\\": "src/"
        }
    },
	"config": {
        "sort-packages": true
    },
	"extra": {
        "laravel": {
			"providers": [
				"AlexGh12\\ChangeLog\\ChangeLogServiceProvider"
			],
			"aliases": {
				"ChangeLog": "AlexGh12\\ChangeLog"
			}
		},
		"branch-alias": {
            "1.x": "1.x-dev"
        }
    }
}
